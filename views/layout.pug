doctype html
head
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  title NFT Talken
  link(rel='shortcut icon' href='assets/images/logo.png')
  link(href='//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0/css/select2.min.css' rel='stylesheet')
  link(href='assets/css/style.css' rel='stylesheet')
  link(href='assets/vendors/datatables/dataTables.bootstrap.min.css' rel='stylesheet')
  link(href='vendor/bootstrap-datetimepicker/bootstrap-datetimepicker.min.css' rel='stylesheet')
  link(href='assets/css/app.min.css' rel='stylesheet')
.app
  .layout
    .header
      .logo.logo-dark
        a(href='/')
          img(src='assets/images/logo.png' alt='Logo')
          img.logo-fold(src='assets/images/logo-fold.png' alt='Logo')
      .logo.logo-white
        a(href='/')
          img(src='assets/images/logo/logo-white.png' alt='Logo')
          img.logo-fold(src='assets/images/logo/logo-fold-white.png' alt='Logo')
      .nav-wrap
        ul.nav-left
          li.desktop-toggle
            a(href='javascript:void(0);')
              i.anticon
          li.mobile-toggle
            a(href='javascript:void(0);')
              i.anticon
        ul.nav-right
          li.dropdown.dropdown-animated.scale-left
            .pointer(data-toggle='dropdown')
              .avatar.avatar-image.m-h-10.m-r-15
                img(src='assets/images/avatars/thumb-3.jpg' alt='')
            .p-b-15.p-t-20.dropdown-menu.pop-profile
              .p-h-20.p-b-15.m-b-10.border-bottom
                .d-flex.m-r-50
                  .avatar.avatar-lg.avatar-image
                    img(src='assets/images/avatars/thumb-3.jpg' alt='')
                  .m-l-10
                    p.m-b-0.admin-name.text-dark.font-weight-semibold.admin-name Marshall Nichols
              a.dropdown-item.d-block.p-h-15.p-v-10(href='/profile')
                .d-flex.align-items-center.justify-content-between
                  div
                    i.anticon.opacity-04.font-size-16.anticon-user
                    span.m-l-10 Edit Profile
                  i.anticon.font-size-10.anticon-right
              a.dropdown-item.d-block.p-h-15.p-v-10.logout-form(href='javascript:void(0);')
                .d-flex.align-items-center.justify-content-between
                  div
                    i.anticon.opacity-04.font-size-16.anticon-logout
                    span.m-l-10 Logout
                  i.anticon.font-size-10.anticon-right
    .side-nav
      .side-nav-inner
        ul.side-nav-menu.scrollable
          li.nav-item.dropdown
            a.dropdown-toggle(href='/')
              span.icon-holder
                i.anticon.anticon-dashboard
              span.title Dashboard
          li.nav-item.dropdown(class='user')
            a.dropdown-toggle(href='/user')
              span.icon-holder
                i.anticon.anticon-team
              span.title User
          li.nav-item.dropdown(class='company')
            a.dropdown-toggle(href='/company')
              span.icon-holder
                i.anticon.anticon-team
              span.title Company             
          li.nav-item.dropdown
            a.dropdown-toggle(href='javascript:void(0);')
              span.icon-holder
                i.anticon.anticon.anticon-inbox
              span.title NFTs
              span.arrow
                i.arrow-icon
            ul.dropdown-menu
              li(class='nft')
                a(href='/nft') NFTs 
              li(class='airdrop')
                a(href='/airdrop') AirDrop
          li.nav-item.dropdown(class='serial')
            a.dropdown-toggle(href='/serials')
              span.icon-holder
                i.anticon.anticon-highlight
              span.title Serials
          li.nav-item.dropdown(class='transaction')
            a.dropdown-toggle(href='/transaction')
              span.icon-holder
                i.anticon.anticon-credit-card
              span.title Transaction
          li.nav-item.dropdown(class='collection')
            a.dropdown-toggle(href='/collection')
              span.icon-holder
                i.anticon.anticon-copyright
              span.title Collection
          li.nav-item.dropdown(class='reward')
            a.dropdown-toggle(href='/reward')
              span.icon-holder
                i.anticon.anticon-gift
              span.title Reward
    .page-container
      .main-content
        block content
      footer.footer
        .footer-content
          p.m-b-0 Copyright &copy; @2021 Talken All rights reserved.
          span
            a.text-gray.m-r-15(href='#') Term &amp; Conditions
            a.text-gray(href='#') Privacy &amp; Policy
    #notification-sending.notification-group.top-right
    #notification-success.notification-group.top-right
    #notification-error.notification-group.top-right
block modal

script(src='assets/js/vendors.min.js')
script(src='assets/js/pagination.js')
script(src='assets/vendors/datatables/jquery.dataTables.min.js')
script(src='assets/vendors/datatables/dataTables.bootstrap.min.js')
script(src='assets/js/pages/datatables.js')
script(src='assets/js/daterangepicker.min.js')
script(src='vendor/bootstrap-datetimepicker/bootstrap-datetimepicker.min.js')
script(src='http://ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js')
script(src='assets/js/api.js')
script(src='assets/js/window.js')
script(src='https://cdn.jsdelivr.net/npm/chart.js@3.5.1/dist/chart.min.js')
block js
script(src='assets/js/app.min.js')
script(src='//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0/js/select2.min.js')
script(src='//cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js')
script(src='//cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.24/moment-timezone.min.js')
script(src='//cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.24/moment-timezone-utils.js' crossorigin='anonymous' referrerpolicy='no-referrer')
script(src='//cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.24/moment-timezone-with-data.js')
